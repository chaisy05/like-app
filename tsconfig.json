{
  "compilerOptions": {
    "target": "esnext",
    "jsx": "react",
    "moduleResolution": "node",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "strict": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "include": [
    "src",
    "app",
    "node_modules/@types/react/index.d.ts"
  ],
  "extends": "expo/tsconfig.base"
}
